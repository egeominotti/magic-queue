{
  "name": "flashq",
  "version": "1.0.0",
  "description": "Official TypeScript SDK for flashQ - High-Performance Job Queue",
  "main": "dist/index.js",
  "module": "src/index.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target bun",
    "typecheck": "tsc --noEmit",
    "example": "bun run examples/01-basic-usage.ts"
  },
  "keywords": [
    "queue",
    "job-queue",
    "task-queue",
    "background-jobs",
    "worker",
    "flashq",
    "bun"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/egeominotti/flashq.git",
    "directory": "sdk/typescript"
  },
  "devDependencies": {
    "typescript": "^5.0.0"
  },
  "optionalDependencies": {
    "@grpc/grpc-js": "^1.14.3",
    "@grpc/proto-loader": "^0.8.0"
  },
  "dependencies": {
    "@msgpack/msgpack": "^3.0.0",
    "bullmq": "^5.66.5",
    "ioredis": "^5.9.2"
  }
}
